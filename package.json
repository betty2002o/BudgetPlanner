  },
  "homepage": "https://github.com/betty2002o/BudgetPlanner#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "mongoose": "^9.0.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
